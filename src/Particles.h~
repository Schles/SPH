#ifndef PARTICLES_H
#define PARTICLES_H

struct Particles {
  std::vector<Eigen::Vector3d> position;
  std::vector<Eigen::Vector3d> position0;
  std::vector<Eigen::Vector3d> velocity;
  std::vector<Eigen::Vector3d> color;

};

struct Boundary : public Particles {
  std::vector<double> m_boundaryPsi;
};


#endif
